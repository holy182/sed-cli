{
  "name": "sed-cli",
  "version": "1.0.5",
  "description": "SED (Semantic Entity Design) - Connect AI to Your Database with Easy Integration",
  "main": "dist/index.js",
  "bin": {
    "sed": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').copyFileSync('src/patterns/business-rules.json', 'dist/patterns/business-rules.json')\"",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "npm audit && npm run lint:security",
    "lint": "eslint src/**/*.ts",
    "lint:security": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc src/index.ts --out docs",
    "sync": "node dist/cli.js sync --force",
    "sync:verbose": "node dist/cli.js sync --force --verbose",
    "validate": "node dist/cli.js validate",
    "discover": "node dist/cli.js build",
    "security:check": "npm audit && npm run test:security",
    "security:fix": "npm audit fix"
  },
  "keywords": [
    "semantic",
    "database",
    "ai",
    "dsl",
    "data-modeling",
    "automation"
  ],
  "author": "SED Team",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/holy182/sed-cli.git"
  },
  "homepage": "https://github.com/holy182/sed-cli#readme",
  "bugs": {
    "url": "https://github.com/holy182/sed-cli/issues"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^12.9.1",
    "mysql2": "^3.6.5",
    "node-cache": "^5.1.2",
    "pg": "^8.11.3",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0"
  }
}
